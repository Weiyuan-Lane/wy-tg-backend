<!DOCTYPE html>
<html>
  <head>
    <title>Weiyuan Backend</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    '/css/bootstrap.min.css' %>
    <%= stylesheet_link_tag    '/css/styles.css' %>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag '/js/lumino.glyphs.js' %>
  </head>
  <body>
    <div class="col-sm-9 col-sm-offset-3 col-lg-10 col-lg-offset-1 main">
		<div class="row">
			<div class="col-lg-12">
        <%= yield %>
      </div>
    </div>
    <%= javascript_include_tag '/js/jquery-1.11.1.min.js' %>
    <%= javascript_include_tag '/js/bootstrap.min.js' %>
  </body>
</html>
